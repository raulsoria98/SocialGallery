{
    "exclude": [
        "node_modules"
    ],
    "compilerOptions": {
        "module": "Node16",
        "baseUrl": "./",
        "paths": {
            "#Config/*": [
                "./src/config/*"
            ],
            "#Controllers/*": [
                "./src/controllers/*"
            ],
            "#Models/*": [
                "./src/models/*"
            ],
            "#Routes/*": [
                "./src/routes/*"
            ],
            "#Validators/*": [
                "./src/validators/*"
            ],
            "#Utils/*": [
                "./src/utils/*"
            ],
            "#DTO/*": [
                "./src/dto/*"
            ],
            "#Middlewares/*": [
                "./src/middlewares/*"
            ]
        }
    }
}