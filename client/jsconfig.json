{
    "exclude": [
        "node_modules"
    ],
    "compilerOptions": {
        "module": "Node16",
        "baseUrl": "./",
        "paths": {
            "#Config/*": [
                "./src/config/*"
            ],
            "#Components/*": [
                "./src/components/*"
            ],
            "#Hooks/*": [
                "./src/hooks/*"
            ],
            "#Pages/*": [
                "./src/pages/*"
            ],
        }
    }
}